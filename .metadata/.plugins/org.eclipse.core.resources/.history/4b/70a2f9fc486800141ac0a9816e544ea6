import java.io.BufferedReader;
import java.io.IOException;
import java.io.InputStreamReader;
import java.util.Vector;


public class Test {

	/**
	 * @param args
	 */
	public static void main(String[] args) {
		Vector<String> words = new Vector<String>();
		Vector<String> arg = new Vector<String>();
		
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		while(true){
			String entry = "";
			
			try {
				entry = br.readLine();
			} catch (IOException ioe) {
				System.out.println("IO error trying to read your name!");
				System.exit(1);
			}
			
			if (entry.equals(".")) break;
			
			words.add(entry);
		}
		
		while(true){
			String entry = "";
			
			try {
				entry = br.readLine();
			} catch (IOException ioe) {
				System.out.println("IO error trying to read your name!");
				System.exit(1);
			}
			
			if (entry.equals(".")) break;
			
			arg.add(entry);
		}
		
		for(int i = 0; i < arg.size(); i++){
			int paramOne, paramTwo;
			String[] nums = arg.elementAt(i).split(" ");
			
			//Gets the parameters.
			paramOne = Integer.parseInt(nums[0]);
			paramTwo = Integer.parseInt(nums[1]);
			
			String format = "{";
			
			for (int j = 0; j < words.size(); j++){
				for (int k = j + 1; k < words.size(); k++){
					//Gets the characters.
					char firstChar;
					char secondChar;
					
					try{
						firstChar = words.elementAt(j).charAt(paramOne);
						secondChar = words.elementAt(k).charAt(paramTwo);
					} catch (Exception e){
						continue;
					}
					
					if (firstChar == secondChar && !(format.contains(String.valueOf((firstChar))))){
						format += format + firstChar + ",";
					}
				}
			}
			
			//Prints the current line.
			format += "}";
			System.out.println(format);
		}
	}

}
